/ default > registrations > edit

#user
  .avatar= image_tag(@user.avatar(:tiny))

  %h1= t(".heading")
  
  = form_for resource, :as => resource_name, :url => registration_path(resource_name), :html => { :method => :put, :multipart => true } do |f|
    = f.error_messages
    = render :partial => "form", :locals => {:f => f}