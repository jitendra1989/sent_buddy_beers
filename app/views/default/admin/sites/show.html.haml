- @title = "Site: #{@site.name}"

= render :partial => "admin/dashboard/tabs"

.tabs
  %ul
    %li= link_to "Back", admin_sites_path
    %li= link_to "Edit Site", edit_admin_site_path(@site)
  .clear

%p
  Name:
  = @site.name
%p
  Subdomain:
  = @site.subdomain

%p
  Code name:
  = @site.code_name

